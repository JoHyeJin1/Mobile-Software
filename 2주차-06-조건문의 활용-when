package ddwu.mobile.week02.kotlintest

fun main(){
    val data : Any = 10
    val result = when(data){
        is Int -> {
            println("Integer!")
            true
        }
        is String -> false
        10 -> true
        in 5..20->true
        else -> false
    }
    println("result : $result")
}

/*
Java의 switch-case문 역할
*/
